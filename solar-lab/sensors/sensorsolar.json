{  
      "method":"getSensorMetadata",
      "sensors":[
         {  
            "sensorId":"ESDval",
            "fullName":"Energy meter",
            "description":"Energy meter data provided",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"input voltage",
                  "unit":"V",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":9.9,
                  "updateFrequency":2
               },
               {  
                  "name":"input current",
                  "unit":"A",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":0.2,
                  "updateFrequency":2
               },
               {  
                  "name":"input wattage",
                  "unit":"W",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":1.98,
                  "updateFrequency":2
               },
               {  
                  "name":"output voltage",
                  "unit":"V",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":9.9,
                  "updateFrequency":2
               },
               {  
                  "name":"output current",
                  "unit":"A",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":0.45,
                  "updateFrequency":2
               },
               {  
                  "name":"output wattage",
                  "unit":"W",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":4.455,
                  "updateFrequency":2
               },
               {  
                  "name":"worktodo",
                  "unit":"J",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":100.0,
                  "updateFrequency":2
               }
            ],
            "accessMode":{  
               "type":"push",
               "nominalUpdateInterval":1000,
               "userModifiableFrequency":false
            }
         },
          {  
            "sensorId":"video",
            "fullName":"video feed",
            "description":"front camera video stream",
            "webSocketType":"binary",
            "singleWebSocketRecommended":true,
            "produces":"image/jpeg",
            "values":[  
               {  
                  "name":"image",
                  "updateFrequency":10
               }
            ],
            "accessMode":{  
               "type":"push",
               "nominalUpdateInterval":100,
               "userModifiableFrequency":false
            }
         }, 
         {  
            "sensorId":"Light",
            "fullName":"Light sensor",
            "description":"Gives the light intensity",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"intensity",
                  "unit":"lm",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":0,
                  "rangeMaximum":700,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         },
         {  
            "sensorId":"PanelTilt",
            "fullName":"Panel tilt angle sensor",
            "description":"Gives the tilt angle of Solar Panel",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"tilt",
                  "unit":"°",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":-90,
                  "rangeMaximum":0,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         },
         {  
            "sensorId":"PanelRot",
            "fullName":"Panel rotation angle sensor",
            "description":"Gives the rotation angle of Solar Panel",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"rotation",
                  "unit":"°",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":-150,
                  "rangeMaximum":150,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         }
      ]
}