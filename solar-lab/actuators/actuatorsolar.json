               {  
                  "name":"tilt",
                  "unit":"°",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":-90,
                  "rangeMaximum":0,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         },
         {  
            "actuatorId":"Sun",
            "fullName":"Sun Brightness",
