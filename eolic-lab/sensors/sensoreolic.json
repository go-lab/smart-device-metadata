{  
      "method":"getSensorMetadata",
      "sensors":[
         {  
            "sensorId":"Doing",
            "fullName":"Experiment in course",
            "description":"Gives the operation being used: Charging (=wind) or Discharging (=wheel)",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"lab",
                  "type":"string",
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         }
         {  
            "sensorId":"ESDval",
            "fullName":"Energy meter",
            "description":"Energy meter data provided",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"input voltage",
                  "unit":"V",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":9.9,
                  "updateFrequency":2
               },
               {  
                  "name":"input current",
                  "unit":"A",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":0.2,
                  "updateFrequency":2
               },
               {  
                  "name":"input wattage",
                  "unit":"W",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":1.98,
                  "updateFrequency":2
               },
               {  
                  "name":"output voltage",
                  "unit":"V",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":9.9,
                  "updateFrequency":2
               },
               {  
                  "name":"output current",
                  "unit":"A",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":0.45,
                  "updateFrequency":2
               },
               {  
                  "name":"output wattage",
                  "unit":"W",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":4.455,
                  "updateFrequency":2
               },
               {  
                  "name":"worktodo",
                  "unit":"J",
                  "type":"float",
                  "rangeMinimum":0.0,
                  "rangeMaximum":100.0,
                  "updateFrequency":2
               }
            ],
            "accessMode":{  
               "type":"push",
               "nominalUpdateInterval":1000,
               "userModifiableFrequency":true
            }
         },
          {  
            "sensorId":"video",
            "fullName":"video feed",
            "description":"front camera video stream",
            "webSocketType":"binary",
            "singleWebSocketRecommended":true,
            "produces":"image/jpeg",
            "values":[  
               {  
                  "name":"image",
                  "updateFrequency":10
               }
            ],
            "configuration":[
               {  
                  "parameter":"compression",
                  "description":"the JPEG compression quality, ranging from 0 (lowest) to 1000 (higher)"
               }
            ],
            "accessMode":{  
               "type":"push",
               "nominalUpdateInterval":100,
               "userModifiableFrequency":false
            }
         }, 
         {  
            "sensorId":"WindPower",
            "fullName":"Tacho fan motor sensor",
            "description":"Gives the fan speed in rpm",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"revolutions",
                  "unit":"m/s",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":0,
                  "rangeMaximum":3230,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         },
         {  
            "sensorId":"MillRot",
            "fullName":"Mill rotation angle sensor",
            "description":"Gives the rotation angle of the Mill",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"rotation",
                  "unit":"°",
                  "type":"int",
                  "rangeStep":1,
                  "rangeMinimum":-150,
                  "rangeMaximum":150,
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         }
         {  
            "sensorId":"LastSeat",
            "fullName":"Last seat crossing stop",
            "description":"Gives the color of the las guy that cross the stop position of the noria",
            "webSocketType":"text",
            "singleWebSocketRecommended":false,
            "produces":"application/json",
            "values":[  
               {  
                  "name":"color",
                  "type":"string",
                  "lastMeasured":"YYYY-MM-DDThh:mm:ssZ"
               }
            ],
            "accessMode":{  
               "type":"pull"
            }
         }
      ]
}